{
  "name": "sound-staff",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=20.19.5"
  },
  "volta": {
    "node": ">=20.19.5"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "yarn eslint && yarn prettier",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "eslint": "ESLINT_USE_FLAT_CONFIG=true npx eslint src --ext .js,.jsx,.ts,.tsx,.astro --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "ESLINT_USE_FLAT_CONFIG=true npx eslint src --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "prettier": "prettier . --check --ignore-unknown",
    "prettier:fix": "prettier . -w",
    "update-node-version": "yarn config set engines.node \"$(node -v | sed 's/v//' | xargs -I {} echo '>={}')\" && yarn config set volta.node \"$(node -v | sed 's/v//' | xargs -I {} echo '>={}')\" && node -v | sed 's/v//' > .nvmrc"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/ts-plugin": "^1.10.4",
    "@eslint/js": "^9.28.0",
    "@iconify/json": "^2.2.391",
    "@iconify/tailwind": "^1.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "astro": "^5.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "fluid-tailwind": "^1.0.4",
    "globals": "^16.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwindcss": "3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
